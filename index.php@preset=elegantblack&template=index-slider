    $(document).ready(function() {
      if ($.cookie('is_unfolded') == '1') {
            $('#presets_selector').removeClass('folded');
            $('#presets_selector').addClass('unfolded');
      }

      $('#optionsbutton').on("click",function() {
        if ($('#presets_selector').hasClass('unfolded')) {
            $('#presets_selector').addClass('folded');
            $('#presets_selector').removeClass('unfolded');
            $.cookie('is_unfolded', '0');
        }
        else {
            $('#presets_selector').removeClass('folded');
            $('#presets_selector').addClass('unfolded');
            $.cookie('is_unfolded', '1');
        }
      });
    });
